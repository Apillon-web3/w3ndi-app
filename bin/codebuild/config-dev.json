{
    "name": "apillon-asset-recipient-dev",
    "environment": {
      "computeType": "BUILD_GENERAL1_SMALL",
      "imagePullCredentialsType": "CODEBUILD",
      "privilegedMode": false,
      "image": "aws/codebuild/amazonlinux2-x86_64-standard:4.0",
      "environmentVariables": [
        {
          "type": "PLAINTEXT",
          "name": "ENV",
          "value": "development"
        },
        {
          "type": "PLAINTEXT",
          "name": "S3_BUCKET",
          "value": "s3://assets-recipient-dev.apillon.io"
        },
        {
          "type": "PLAINTEXT",
          "name": "CLOUDFRONT_ID",
          "value": "E3K889SGGIMDD2"
        },
        {
            "type": "PLAINTEXT",
            "name": "NODE_OPTIONS",
            "value": "--max_old_space_size=4096"
        }
      ],
      "type": "LINUX_CONTAINER"
    },
    "timeoutInMinutes": 60,
    "serviceRole": "arn:aws:iam::018021943180:role/atv2-deployer",
    "artifacts": {
      "type": "NO_ARTIFACTS"
    },
    "cache": {
      "type": "NO_CACHE"
    },
    "sourceVersion": "develop",
    "source": {
      "insecureSsl": false,
      "gitSubmodulesConfig": {
        "fetchSubmodules": false
      },
      "location": "https://TineMlakar@bitbucket.org/kalmiadevs/apillon-kilt-asset-recipient.git",
      "gitCloneDepth": 1,
      "type": "BITBUCKET",
      "reportBuildStatus": false
    },
    "badgeEnabled": true,
    "queuedTimeoutInMinutes": 480,
    "logsConfig": {
      "s3Logs": {
        "status": "DISABLED",
        "encryptionDisabled": false
      },
      "cloudWatchLogs": {
        "status": "ENABLED",
        "groupName": "codebuild",
        "streamName": "apillon-asset-recipient-dev"
      }
    },
    "encryptionKey": "arn:aws:kms:eu-west-1:018021943180:alias/aws/s3"
  }
